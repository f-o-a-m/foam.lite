{
  "name": "foam5g",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "merge-contracts-for-etherscan": "rm -rf contracts/merged && sol-merger \"./contracts/*.sol\" \"./contracts/merged\"",
    "postinstall": "spago -x contracts.dhall build",
    "chanterelle-build": "chanterelle build",
    "deploy-contracts": "chanterelle deploy output/DApp.Deploy.Script/index.js",
    "build-app": "spago -x spago.dhall build",
    "build-dapp": "spago -x dapp.dhall build",
    "build-server": "spago -x server.dhall build",
    "run-server": "spago -x server.dhall run",
    "test-dapp": "spago -x test.dhall test",
    "bundle-app": "spago -x spago.dhall bundle-app --to dist/bundle.js",
    "browserify": "browserify dist/bundle.js -o dist/bundle.js -t envify",
    "serve": "serve dist",
    "webpack": "DEBUG=purs-loader* DEBUG_DEPTH=100 webpack --mode development --progress --bail",
    "webpack:watch": "DEBUG=purs-loader* DEBUG_DEPTH=100 webpack --mode development --progress --display-error-details --display verbose --watch",
    "webpack:server": "webpack-dev-server --mode development --progress --inline --hot",
    "webpack:server:debug": "DEBUG=purs-loader* DEBUG_DEPTH=100 webpack-dev-server --mode development --progress --inline --hot",
    "bundle-helper": "spago -x helper.dhall bundle-app --to dist/helper.js",
    "bundle-server": "spago -x server.dhall bundle-app --to dist/server.js",
    "backend-dist": "npm run --silent backend-package-json > dist/package.json && npm run bundle-helper && npm run bundle-server",
    "backend-package-json": "node -p \"let p=require('./package');delete p.devDependencies;delete p.scripts;JSON.stringify(p,{},4);\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@luma.gl/engine": "^8.3.1",
    "@luma.gl/webgl": "^8.3.1",
    "@openzeppelin/contracts": "^3.2.0",
    "create-react-class": "^15.6.3",
    "deck.gl": "^8.2.8",
    "ethjs-provider-http": "^0.1.6",
    "luma.gl": "^7.3.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-map-gl": "^5.2.8",
    "secp256k1": "^3.0.0",
    "sol-merger": "^3.1.0",
    "solc": "^0.6",
    "web3": "^1.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "browserify": "^17.0.0",
    "chanterelle": "github:f-o-a-m/chanterelle#v5.0.0",
    "envify": "^4.1.0",
    "html-webpack-plugin": "^4.5.0",
    "parcel": "^1.12.4",
    "pulp": "^15.0.0",
    "purescript": "^0.13.0",
    "purescript-psa": "^0.8.0",
    "purs-loader": "^3.7.2",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "shader-loader": "^1.3.1",
    "spago": "^0.16.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  }
}
